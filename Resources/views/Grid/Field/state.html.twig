{% set value = 'miky.ui.' ~ data %}

{% if options.vars.labels is defined %}
    {% include [(options.vars.labels ~ ':' ~ data ~ '.html.twig'), '@MikyAdmin/Label/_default.html.twig'] with {'value': value} %}
{% else %}
    {% include '@MikyAdmin/Label/_default.html.twig' with {'value': value} %}
{% endif %}

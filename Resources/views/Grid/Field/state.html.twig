{% set value = 'adevis.ui.' ~ data %}

{% if options.vars.labels is defined %}
    {% include [(options.vars.labels ~ ':' ~ data ~ '.html.twig'), '@AdevisAdmin/Label/_default.html.twig'] with {'value': value} %}
{% else %}
    {% include '@AdevisAdmin/Label/_default.html.twig' with {'value': value} %}
{% endif %}
